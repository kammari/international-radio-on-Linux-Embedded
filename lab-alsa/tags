BLOCK_SIZE	pcm.c	10;"	d	file:
CHANNELS	pcm.c	5;"	d	file:
LATENCY	pcm.c	7;"	d	file:
NBLOCKS	pcm.c	9;"	d	file:
SAMPLE_RATE	pcm.c	3;"	d	file:
buffer	pcm.c	/^unsigned char buffer[NBLOCKS*BLOCK_SIZE];     $/;"	v
device	pcm.c	/^static char *device = "default";   \/* playback device *\/$/;"	v	file:
main	pcm.c	/^int main(int argc, char ** argv)$/;"	f
